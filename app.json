{
  "expo": {
    "name": "SnapBuy",
    "slug": "snapbuy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app-icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#5B5FFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.snapbuy.app",
      "build": {
        "channels": ["production"]
      }
    },
    "android": {
      "package": "com.snapbuy.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/logo.png",
        "backgroundColor": "#5B5FFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/app-icons/logo.png"
    },
    "description": "SnapBuy - Your favorite online shopping app",
    "primaryColor": "#5B5FFF",
    "assetBundlePatterns": ["**/*"]
  }
}
